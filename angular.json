{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm"
  },
  "newProjectRoot": "projects",
  "projects": {
    "laydate-demo": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/laydate-demo",
      "sourceRoot": "projects/laydate-demo/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/laydate-demo/src/main.ts",
            "tsConfig": "projects/laydate-demo/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/laydate-demo/public"
              }
            ],
            "styles": [
              "projects/laydate-demo/src/styles.scss"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "laydate-demo:build:production"
            },
            "development": {
              "buildTarget": "laydate-demo:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular/build:unit-test"
        }
      }
    },
    "ng-laydate": {
      "projectType": "library",
      "root": "projects/ng-laydate",
      "sourceRoot": "projects/ng-laydate/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ng-laydate/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-laydate/tsconfig.lib.json"
            },
            "development": {
              "tsConfig": "projects/ng-laydate/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ng-laydate/tsconfig.spec.json",
            "karmaConfig": "projects/ng-laydate/karma.conf.js"
          }
        }
      }
    }
  }
}